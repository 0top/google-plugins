{
	"manifest_version": 2,
	"name": "csdn cleaner",
	"version": "2.0.0",
	"description": "adv killer",
	"icons":
	{
		"16": "img/icon.png",
		"48": "img/icon.png",
		"128": "img/icon.png"
	},
	"browser_action": 
	{
		"default_icon": "img/icon.png",
		"default_title": "csdn adv killer",
		"default_popup": "popup.html"
	},
	"content_scripts": 
	[
		{
			"matches": ["https://*.csdn.net/*"],
			"js": ["js/jquery.js", "js/corejs.js"],
			"run_at": "document_start"
		},
		{
			"matches": ["https://*.csdn.net/*"],
			"js": ["js/jquery.js", "js/core_end.js"],
			"run_at": "document_end"
		},
		{
			"matches": ["<all_urls>"],
			"js": ["js/jquery.js", "js/corejs.js"],
			"run_at": "document_end"
		},
		{
			"matches": ["https://www.regular-expressions.info/*"],
			"js": ["js/jquery.js", "js/core_regular_expression.js"],
			"run_at": "document_start"
		}
	],
	"homepage_url": "https://0top.github.io/"
}
